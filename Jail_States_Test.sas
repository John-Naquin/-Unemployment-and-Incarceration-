/* 1. One-Way ANOVA for Jail Population */
proc glm data=state_data;
    class state;
    model jail = state;
    means state / tukey;
    title "One-Way ANOVA for Jail Population by State";
run;
quit;

/* 1. One-Way ANOVA for Unemployment Rate */
proc glm data=state_data;
    class state;
    model unemployed = state;
    means state / tukey;
    title "One-Way ANOVA for Unemployment Rate by State";
run;
quit;

/* 2. Normality Test and QQ Plot for Jail Population */
proc univariate data=state_data normal;
    var jail;
    histogram jail / normal;
    qqplot jail / normal(mu=est sigma=est);
    title "Normality Test and QQ Plot for Jail Population";
run;

/* 2. Normality Test and QQ Plot for Unemployment Rate */
proc univariate data=state_data normal;
    var unemployed;
    histogram unemployed / normal;
    qqplot unemployed / normal(mu=est sigma=est);
    title "Normality Test and QQ Plot for Unemployment Rate";
run;

/* 3. Levene's Test for Homogeneity of Variances (Jail Population) */
proc glm data=state_data;
    class state;
    model jail = state;
    means state / hovtest=levene;
    title "Levene's Test for Homogeneity of Variances (Jail Population)";
run;
quit;

/* 3. Levene's Test for Homogeneity of Variances (Unemployment Rate) */
proc glm data=state_data;
    class state;
    model unemployed = state;
    means state / hovtest=levene;
    title "Levene's Test for Homogeneity of Variances (Unemployment Rate)";
run;
quit;

/*4. Kruskal-Wallis Test for Jail Population */
proc npar1way data=state_data wilcoxon;
    class state;
    var jail;
    title "Kruskal-Wallis Test for Jail Population by State";
run;

/*4. Kruskal-Wallis Test for Unemployment Rate */
proc npar1way data=state_data wilcoxon;
    class state;
    var unemployed;
    title "Kruskal-Wallis Test for Unemployment Rate by State";
run;

/* Convert Unemployment Rate into Categorical Variable */
proc rank data=state_data out=state_data_ranks groups=3;
    var unemployed;
    ranks unemp_cat;
run;

/* Adjust Category Labels */
data state_data_ranks2;
    set state_data_ranks;
    unemp_cat = unemp_cat + 1; /* Categories: 1, 2, 3 */
run;

/* Convert Jail Population into Categorical Variable */
proc rank data=state_data out=state_data_jail_ranks groups=3;
    var jail;
    ranks jail_cat;
run;

/* Adjust Category Labels */
data state_data_jail_ranks2;
    set state_data_jail_ranks;
    jail_cat = jail_cat + 1; /* Categories: 1, 2, 3 */
run;


/* Chi-Square Test for Independence between State and Unemployment Category */
proc freq data=state_data_ranks2;
    tables state*unemp_cat / chisq expected;
    title "Chi-Square Test for Independence between State and Unemployment Category";
run;

/* Chi-Square Test for Independence between State and Jail Category */
proc freq data=state_data_jail_ranks2;
    tables state*jail_cat / chisq expected;
    title "Chi-Square Test for Independence between State and Jail Category";
run;


/* Correlation Analysis Between Jail Population and Unemployment Rate */
proc corr data=state_data;
    var jail unemployed;
    title "Correlation Between Jail Population and Unemployment Rate";
run;






